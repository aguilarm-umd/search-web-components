{
  "name": "drupal-search-web-components",
  "version": "1.0.0-alpha.6",
  "description": "A library of Lit web components to use with the search_api_decoupled module.",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://git.drupalcode.org/project/search_web_components"
  },
  "keywords": [
    "drupal",
    "search",
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "scripts": {
    "build": "rimraf dist/* && tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist/* test/my-element.{d.ts,d.ts.map,js,js.map} test/my-element_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\" --exclude \"src/BaseSearchElement.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\"  --exclude \"src/BaseSearchElement.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "bundle": "npm run build && rollup -c",
    "checksize": "rollup -c ; cat dist/search-web-components.min.js | gzip -9 | wc -c ;",
    "storybook": "npm run analyze && storybook dev -p 6006",
    "storybook:build": "npm run analyze && BASE_URL=/search_web_components/ storybook build",
    "deploy": "npm run build; npm run bundle; npm run analyze; npm run storybook:build",
    "release:dev": "release-it --preRelease=dev --npm.tag=10x --no-git.requireCleanWorkingDir --no-git.tag --git.commitMessage=\"CI: JS bundle for commit $CI_COMMIT_SHORT_SHA\" --git.pushArgs=\"-o ci.skip\" --plugins.@release-it/bumper.out.path=dev --plugins.@release-it/bumper.in.path=dev",
    "release:alpha": "release-it prerelease --preRelease=alpha --no-git.requireCleanWorkingDir --git.pushArgs=\"-o ci.skip --follow-tags\" --plugins.@release-it/bumper.out.path=alpha --plugins.@release-it/bumper.in.path=alpha",
    "release:beta": "release-it prerelease --preRelease=beta --no-git.requireCleanWorkingDir --git.pushArgs=\"-o ci.skip --follow-tags\" --plugins.@release-it/bumper.out.path=beta --plugins.@release-it/bumper.in.path=beta",
    "release:patch": "release-it patch --no-git.requireCleanWorkingDir --git.pushArgs=\"-o ci.skip --follow-tags\" --git.tagExclude=\"*[-]*\" --plugins.@release-it/bumper.out.path=rel --plugins.@release-it/bumper.in.path=rel",
    "release:minor": "release-it minor --no-git.requireCleanWorkingDir --git.pushArgs=\"-o ci.skip --follow-tags\" --git.tagExclude=\"*[-]*\" --plugins.@release-it/bumper.out.path=rel --plugins.@release-it/bumper.in.path=rel",
    "release:major": "release-it major --no-git.requireCleanWorkingDir --git.pushArgs=\"-o ci.skip --follow-tags\" --git.tagExclude=\"*[-]*\" --plugins.@release-it/bumper.out.path=rel --plugins.@release-it/bumper.in.path=rel",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "files": [
    "src",
    "dist",
    "search-web-components.min.js",
    "custom-elements.json"
  ],
  "dependencies": {
    "@lit/context": "^1.1.0",
    "@search-api-decoupled/client": "^1.1.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@open-wc/testing": "^3.0.0-next.5",
    "@release-it/bumper": "^6.0.1",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/blocks": "^7.6.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/web-components": "^7.6.8",
    "@storybook/web-components-vite": "^7.6.8",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@wdio/browser-runner": "^8.40.2",
    "@wdio/cli": "^8.40.2",
    "@wdio/junit-reporter": "^8.40.0",
    "@wdio/mocha-framework": "^8.40.2",
    "@wdio/spec-reporter": "^8.39.0",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.15.0",
    "eslint-plugin-storybook": "^0.6.15",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "lit": "^3.2.0",
    "lit-analyzer": "^1.2.1",
    "mocha": "^10.3.0",
    "msw": "^1.3.2",
    "msw-storybook-addon": "^1.10.0",
    "prettier": "^2.6.2",
    "release-it": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^23.0.0",
    "storybook": "^7.6.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.0",
    "wdio-wait-for": "^3.0.11",
    "wdio-wiremock-service": "^7.0.2"
  },
  "customElements": "custom-elements.json",
  "msw": {
    "workerDirectory": "public"
  },
  "release-it": {
    "plugins": {
      "./utils/ReleaseItPlugins.js": {},
      "@release-it/bumper": {
        "in": {
          "file": "version.json",
          "type": "application/json"
        },
        "out": {
          "file": "version.json",
          "type": "application/json"
        }
      }
    }
  }
}
